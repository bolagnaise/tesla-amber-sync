# .env.example
# Copy this file to .env and fill in your actual values

# Flask
SECRET_KEY=your-super-secret-and-long-random-string-change-this
FLASK_RUN_PORT=5001

# Database (SQLite is used by default - this PostgreSQL URL is optional)
DATABASE_URL=postgresql://your_db_user:your_db_password@localhost:5432/teslasync

# Encryption Key (GENERATE A NEW ONE - DO NOT USE THIS EXAMPLE)
# Run this in a python shell: from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())
# And paste the result here.
FERNET_ENCRYPTION_KEY=GENERATE_YOUR_OWN_KEY_DO_NOT_USE_THIS_EXAMPLE

# Tesla API Credentials
# Get these from https://developer.tesla.com/
TESLA_CLIENT_ID=your-tesla-client-id-from-developer-portal
TESLA_CLIENT_SECRET=ta-secret.your-tesla-client-secret
TESLA_REDIRECT_URI=http://localhost:5001/tesla-fleet/callback
APP_DOMAIN=http://localhost:5001
